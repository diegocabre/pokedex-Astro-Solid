{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@layouts/*": [
        ".src/layouts/*"
      ],
      "@components/*": [
        ".src/components/*"
      ],
      "@interfaces/*": [
        ".src/pages/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}